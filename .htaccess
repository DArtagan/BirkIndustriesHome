# prevent viewing of a github.php file
<files github.php>
 order deny,allow
 deny from all
 allow from github.com
</files>

# BEGIN WordPress
<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /
RewriteRule ^index\.php$ - [L]
RewriteRule ^css/(.*) /wp-content/themes/retlehs-roots-2b9f334/css/$1 [QSA,L]
RewriteRule ^js/(.*) /wp-content/themes/retlehs-roots-2b9f334/js/$1 [QSA,L]
RewriteRule ^img/(.*) /wp-content/themes/retlehs-roots-2b9f334/img/$1 [QSA,L]
RewriteRule ^plugins/(.*) /wp-content/plugins/$1 [QSA,L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /index.php [L]
</IfModule>

# END WordPress
